{
  "name": "steganologger",
  "displayName": "Steganologger",
  "description": "An extension to decode information encoded in plots using the steganologger python script",
  "icon": "media/steganologger-icon.png",
  "version": "0.0.2",
  "repository": "https://github.com/juupje/steganologger",
  "publisher": "juupje",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "Steganography",
    "Plot logging"
  ],
  "preview": true,
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "steganologger.showInfo",
        "title": "Decode image",
        "when": "false"
      },
      {
        "command": "steganologger.clear",
        "title": "Steganologger: Clear tabs",
        "icon": "$(notebook-clear)"
      },
      {
        "command": "steganologger.removeTab",
        "title": "Steganologger: Remove current tab",
        "icon": "$(notebook-delete-cell)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname in steganologger.supportedExtensions",
          "command": "steganologger.showInfo"
        }
      ],
      "view/title": [
        {
          "when": "view == steganologger.webview",
          "command": "steganologger.clear",
          "group": "navigation"
        },
        {
          "when": "view == steganologger.webview",
          "command": "steganologger.removeTab",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "panel": [
        {
          "id": "steganologger-container",
          "title": "Steganologger",
          "icon": "media/steganologger-icon.png"
        }
      ]
    },
    "views": {
      "steganologger-container": [
        {
          "type": "webview",
          "id": "steganologger.webview",
          "name": "Decoded"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node ./esbuild.js",
    "package": "NODE_ENV=production node ./esbuid.js",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@types/vscode-webview": "^1.57.1",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "esbuild": "^0.16.17",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.2.1",
    "npm": "^9.5.0",
    "png-js": "^1.0.0"
  }
}
